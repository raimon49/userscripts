{
  "rules": {
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-str": 2,
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "space-before-function-paren": [
      2,
      {
        "anonymous": "ignore",
        "named": "never"
      }
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "no-trailing-spaces": 2,
    "curly": [
      2,
      "all"
    ],
    "no-multiple-empty-lines": 2,
    "eol-last": 2,
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ]
  },
  "parserOptions": {
      "ecmaVersion": 2015

  }
}
